<div id="Banner">
    @*<img style="min-height:300px; min-width:1079.2px;" src="Content/Image/Banner/Banner-img.jpg" />*@
    <img style="height:275px; width:100%" src='@Url.Content("~/Uploads/todoku/Banner/Banner-img.jpg")' />
