<div class="ProductList">
    <div class="ContentTitle">Product List</div>
    <div>
        <ul>
            @foreach (var item in (List<Todoku.Models.StandardCode>)ViewBag.Categories) 
            { 
                <li style="display:inline-block;">
                    <div class="category">
                        @Html.ActionLink(@item.StandardCodeName, "Index", "Stores", new { category = item.StandardCodeID }, new { area = "Stores" })
                    </div>
                </li>
            }
        </ul>
    </div>
</div>

<div class="HotList">
    <div class="ContentTitle">Hot List</div>
    @foreach (var obj in Model) 
    { 
        @Html.DisplayFor(modelItem => obj);
    }
</div>
